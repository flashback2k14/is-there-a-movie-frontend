{
  "name": "is-there-a-movie-frontend",
  "description": "Frontend for is-there-a-movie",
  "private": true,
  "scripts": {
    "build": "polymer build --entrypoint app/index.html",
    "clean": "rm -rf build node_modules",
    "deploy": "npm run lint && gulp deploy",
    "lint": "polymer lint",
    "serve": "polymer serve -o -p 8088",
    "serve-bundled": "cd build/bundled/ && npm run serve",
    "serve-unbundled": "cd build/unbundled/ && npm run serve",
    "test": "polymer test",
    "test-skip-si": "polymer test --skip-selenium-install"
  }
}